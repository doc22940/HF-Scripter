    "build-client": "webpack --config ./hifi-script/webpack.config.js -p",
    "copy-assets": "cpy hf-scripter-client.js tableticon.svg ./dist/"
  },
  "author": "jjv360",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
