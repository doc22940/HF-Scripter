    "promise-polyfill": "^7.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-toolbox": "^2.0.0-beta.12",
    "uuid": "^3.2.1"
  }
