{
  "name": "hf-scripter",
  "version": "1.0.0",
  "description": "Allows visually adding scripts to entities.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-old": "webpack-dev-server --watch --progress --open",
    "dev": "npm run build && http-serve ./dist/ -c-1",
